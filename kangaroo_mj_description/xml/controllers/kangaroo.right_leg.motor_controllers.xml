<mujoco model="kangaroo_right_leg_motor_controllers">
	<compiler angle="radian"/>
	
	<actuator>
		<motor name="leg_right_1_actuator_torque" joint="leg_right_1_joint" ctrllimited="true" ctrlrange="-80. 80."/> <!-- YAW Nm -->
		<motor name="leg_right_2_actuator_torque" joint="leg_right_2_joint" ctrllimited="true" ctrlrange="-230. 230."/> <!-- Pitch Nm -->
		<motor name="leg_right_3_actuator_torque" joint="leg_right_3_joint" ctrllimited="true" ctrlrange="-139. 139."/> <!-- Roll Nm -->
		<motor name="leg_right_length_actuator_torque" joint="leg_right_length_joint" ctrllimited="true" ctrlrange="-1100. 1100."/> <!-- N Force. NOT TORQUE! -->
		<motor name="leg_right_4_actuator_torque" joint="leg_right_4_joint" ctrllimited="true" ctrlrange="-140. 140."/> <!-- Ankle Pitch Nm -->
		<motor name="leg_right_5_actuator_torque" joint="leg_right_5_joint" ctrllimited="true" ctrlrange="-82. 82."/> <!-- Ankle Roll Nm -->
  	</actuator>

</mujoco>
